{
  "name": "frontend-fiscales",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint src --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "preview": "tsc && vite build && vite preview",
    "preview-stg": "tsc && vite build --mode staging && vite preview",
    "server": "json-server --watch ./src/mocks/db.json --port 5000",
    "i18next-resources-for-ts": "i18next-resources-for-ts toc -i ./src/i18n/en -o ./src/@types/resources.ts",
    "precommit": "lint-staged",
    "prepare": "husky install",
    "fix": "yarn fix:eslint && yarn fix:prettier",
    "fix:eslint": "eslint --fix \"**/*.*\"",
    "fix:prettier": "prettier --write \"**/*.*\""
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn precommit"
    }
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@headlessui/react": "^1.7.17",
    "@mui/material": "^5.14.16",
    "@phosphor-icons/react": "^2.0.14",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-select": "^2.0.0",
    "@react-hook/media-query": "^1.1.1",
    "@types/react-router-dom": "^5.3.3",
    "axios": "^1.5.0",
    "browser-image-compression": "^2.0.2",
    "class-variance-authority": "^0.7.0",
    "classnames": "^2.3.2",
    "clsx": "^2.0.0",
    "dotenv": "^16.3.1",
    "firebase": "^10.6.0",
    "formik": "^2.4.3",
    "framer-motion": "^10.16.4",
    "husky": "^8.0.3",
    "i18next": "^23.7.6",
    "jwt-decode": "3.1.2",
    "lint-staged": "^15.1.0",
    "mitt": "^3.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hot-toast": "^2.4.1",
    "react-i18next": "^13.5.0",
    "react-router-dom": "^6.17.0",
    "sharp": "^0.32.6",
    "svgo": "^3.0.2",
    "tailwind-merge": "^2.0.0",
    "tailwindcss-animate": "^1.0.7",
    "yup": "^1.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react-swc": "^3.3.2",
    "autoprefixer": "^10.4.15",
    "concurrently": "^8.2.2",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "i18next-resources-for-ts": "^1.4.0",
    "json-server": "^0.17.4",
    "prettier": "^3.1.0",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.0.2",
    "vite": "^4.4.5",
    "vite-plugin-auto-alias": "^0.2.11",
    "vite-plugin-dynamic-import": "^1.5.0",
    "vite-plugin-image-optimizer": "^1.1.7",
    "vite-plugin-minify": "^1.5.2",
    "vite-plugin-obfuscator": "^1.0.5"
  }
}
